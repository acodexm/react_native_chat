{
  "name": "react-native-chat",
  "version": "0.0.1",
  "private": true,
  "peferGlobal": false,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-o": "jest -o",
    "test-u": "jest -u",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write '**/*.js'",
    "format:json": "prettier --write '**/*.json'",
    "format:md": "prettier --write '**/*.md'",
    "format:ts": "prettier --write '**/*.ts{,x}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "g": "plop",
    "postinstall": "npx jetify",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "link:fonts": "npx react-native link"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-native-firebase/analytics": "^7.6.9",
    "@react-native-firebase/app": "^8.4.7",
    "@react-native-firebase/auth": "^9.3.2",
    "@react-native-firebase/crashlytics": "^8.4.12",
    "@react-native-firebase/dynamic-links": "^7.5.11",
    "@react-native-firebase/firestore": "^7.9.1",
    "@react-native-firebase/messaging": "^7.9.2",
    "@react-native-firebase/perf": "^7.4.10",
    "@react-native-firebase/remote-config": "^9.0.12",
    "@react-native-firebase/storage": "^7.4.11",
    "@react-navigation/bottom-tabs": "^5.10.4",
    "@react-navigation/native": "^5.8.4",
    "@react-navigation/stack": "^5.12.1",
    "formik": "^2.2.1",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-native": "^0.63.3",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-iphone-x-helper": "^1.3.0",
    "react-native-localize": "^1.4.3",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.12.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^10.10.0",
    "rxfire": "^4.0.0",
    "rxjs": "^6.6.3",
    "rxjs-hooks": "^0.6.2",
    "styled-components": "^5.2.1",
    "typedjson": "^1.6.0-rc2",
    "use-constant": "^1.0.0",
    "uuid": "^8.3.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/react": "^6.0.28",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/react-native": "^7.1.0",
    "@types/eslint": "^7.2.4",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/prettier": "^2.1.5",
    "@types/react-native": "^0.63.32",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "16.9.3",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unused-imports": "^1.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.1",
    "metro-react-native-babel-preset": "^0.63.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.4",
    "prettier": "^2.1.2",
    "react-native-dotenv": "^2.4.2",
    "react-native-get-random-values": "^1.5.0",
    "react-native-logs": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "jest -o"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint src/**/*.{js,ts,tsx,jsx} --fix-dry-run"
    ]
  }
}
